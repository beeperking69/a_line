all:
	@make --no-print-directory -f generic.make TGT=linear_algebra all
	@make --no-print-directory -f generic.make TGT=radix all

	@make --no-print-directory -f generic.make TGT=linear_algebra static
	@make --no-print-directory -f generic.make TGT=radix static

clean:
	@make --no-print-directory -f generic.make TGT=linear_algebra clean
	@make --no-print-directory -f generic.make TGT=radix clean
